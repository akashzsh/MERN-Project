{
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 0,
    "no-console": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
